<view><view class="nav-wrap"><block wx:for="{{goodsTabsVoList}}" wx:for-item="item" wx:for-index="unique" wx:key="unique"><block><view data-event-opts="{{[['tap',[['chooseNav',[unique]]]]]}}" class="{{[chooseNavIndex==unique?'choose':'']}}" bindtap="__e">{{item.tabName}}</view></block></block></view><view class="jingxuan-main"><block wx:for="{{currentgoodsVo}}" wx:for-item="item" wx:for-index="c" wx:key="c"><block wx:if="{{currentgoodsVo.length!=0}}"><block><view data-event-opts="{{[['tap',[['commodity_details',['$0'],[[['currentgoodsVo','',c,'id']]]]]]]}}" class="jingxuan-main-box" bindtap="__e"><view><image src="{{item.mainImg}}"></image></view><view><text>{{item.goodsName}}</text><view><text>￥</text><text class="goodsListMoney1">{{item.mainPriceInteger+"."}}</text><text class="goodsListMoney2">{{item.mainPriceDecimal}}</text></view></view></view></block></block></block><view hidden="{{!(isloadedend)}}" class="bottomText">已全部加载</view></view></view>