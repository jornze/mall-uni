<view><view class="swiper-box"><swiper current="0" circular="true" data-event-opts="{{[['change',[['swiperChange',['$event']]]]]}}" bindchange="__e"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="idx" wx:key="idx"><swiper-item><image src="{{item}}" data-event-opts="{{[['tap',[['previewImage1',[idx]]]]]}}" bindtap="__e"></image></swiper-item></block></swiper><view class="imageCount">{{current+1+"/"+list.length}}</view></view><view class="commodity-content"><view><view><text><text>￥</text>{{chooseSku.marketPrice}}</text><block wx:if="{{chooseSku.marketPrice!==chooseSku.retailPrice}}"><text>{{"￥"+chooseSku.retailPrice}}</text></block></view><view><block wx:for="{{all.ensureList}}" wx:for-item="item" wx:for-index="a" wx:key="a"><block wx:if="{{item.rank==1}}"><text>{{item.content}}</text></block></block></view></view><view><view><view><text class="commodity-title">{{all.goodsName}}</text><text class="commodity-class">定制版</text></view><view><block wx:for="{{all.ensureList}}" wx:for-item="item" wx:for-index="a" wx:key="a"><block wx:if="{{item.rank==2}}"><text class="commodity-ensure">{{item.content}}</text></block></block></view></view><view><image src="/static/images/Commodity_details_Collection_uncheckede_Copy.png"></image><text>已收藏</text></view></view></view><view class="commodity-image"><block wx:for="{{detailImgList}}" wx:for-item="item" wx:for-index="d" wx:key="d"><block><image src="{{item}}" mode="widthFix"></image></block></block></view><view class="commodity-bottom"><view><view><image src="/static/images/Commodity_details_Private_letter.png"></image><text>私信</text></view><view data-event-opts="{{[['tap',[['shoppingCart',['$event']]]]]}}" bindtap="__e"><image src="/static/images/Commodity_details_shopping_Copy.png"></image><text>购物车</text><block wx:if="{{shoppingCartNum}}"><view>{{shoppingCartNum}}</view></block></view></view><view><text data-event-opts="{{[['tap',[['shoppingCartChoose',['$event']]]]]}}" bindtap="__e">加入购物车</text><text data-event-opts="{{[['tap',[['buyChoose',['$event']]]]]}}" bindtap="__e">立即购买</text></view></view><block wx:if="{{choose}}"><view class="commodity-choose"><view class="commodity-choose-intro"><view><image src="{{chooseSku.mainImg}}" data-event-opts="{{[['tap',[['previewImage2',['$0'],['chooseSku.mainImg']]]]]}}" bindtap="__e"></image></view><view><text class="commodity-choose-intro-name">{{all.goodsName}}</text><text class="commodity-choose-intro-class">{{"已选："+chooseSku.skuName+"；"}}</text><text class="commodity-choose-intro-free">赠品：皮绳红色</text><view class="commodity-choose-intro-money"><view><label class="_span">￥</label>{{chooseSku.marketPrice}}</view><text>{{"x"+chooseSku.skuQty}}</text></view></view><image class="close" src="/static/images/close.png" data-event-opts="{{[['tap',[['close',['$event']]]]]}}" bindtap="__e"></image></view><view class="commodity-choose-guige"><text>规格</text><view><block wx:for="{{goodsSkuVoList}}" wx:for-item="item" wx:for-index="g" wx:key="g"><block><block wx:if="{{!item.skuStock==0}}"><text data-event-opts="{{[['tap',[['chooseType',[g]]]]]}}" class="{{[chooseIndex==g?'commodity-choose-guige-choose':'commodity-choose-guige-unchoose']}}" bindtap="__e">{{item.skuName}}</text></block><block wx:if="{{item.skuStock==0}}"><text data-event-opts="{{[['tap',[['chooseType',[g]]]]]}}" class="{{[chooseIndex==g?'commodity-choose-guige-choose':'commodity-choose-guige-unchoose2']}}" bindtap="__e">{{item.skuName}}</text></block></block></block></view></view><view class="commodity-choose-num"><text>数量</text><view class="buy-num"><view data-event-opts="{{[['tap',[['reduce',['$event']]]]]}}" class="{{[chooseSku.skuQty==1?'jian-btn-disabled':'jian-btn']}}" bindtap="__e">－</view><input disabled="true" value="{{chooseSku.skuQty}}"/><view data-event-opts="{{[['tap',[['add',['$event']]]]]}}" class="{{[chooseSku.skuQty==chooseSku.skuStock?'jia-btn-disabled':'jia-btn']}}" bindtap="__e">+</view></view></view><view class="commodity-choose-free"><text>赠品</text><view><text class="commodity-choose-guige-unchoose">红色</text><text class="commodity-choose-guige-unchoose">黑色</text><text class="commodity-choose-guige-unchoose">随机</text></view></view><block wx:if="{{choose=='shoppingCart'&&chooseSku.skuStock!==0}}"><view data-event-opts="{{[['tap',[['joinShoppingCart',['$event']]]]]}}" class="commodity-choose-bottom" bindtap="__e">加入购物车</view></block><block wx:if="{{chooseSku.skuStock==0}}"><view class="commodity-choose-bottom2">库存不足</view></block><block wx:if="{{choose=='buy'&&chooseSku.skuStock!==0}}"><view data-event-opts="{{[['tap',[['orderDetails',['$event']]]]]}}" class="commodity-choose-bottom" bindtap="__e">选好了</view></block></view></block></view>